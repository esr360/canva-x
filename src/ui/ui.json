{
    "ui": {
        "asset-path": "'../..'",
        "sass-config-parser": "eval-config",
        "component-glue": "'_'",
        "modifier-glue": "'-'",
        "theme": "One-Nexus",
        "css-class-props": ["object"],
        "modules": {
            "foo": ["#COLOR", "brand", "brand-5"],
            "colors": {
                "brand": {
                    "brand-1": "pink",
                    "brand-5": "aqua"
                }
            },
            "accordion": {
                "foo": "bar"
            },
            "button": {
                "palettes": [
                    "brand", "greyscale", "alert", "social", 
                    {"foo": "blue", "bar": "#FF5733"}
                ],
                "combine": {
                    "primary" : ["round", "size-4", "brand-1"],
                    "social"  : ["icon", "oval", "size-6", "brand-2"]
                }
            },
            "modal": {
                "buzz": true,
                "top-position": "30%"
            },
            "carousel": {
                "navigationItem": {
                    "disable": false
                }
            }
        }
    }
}